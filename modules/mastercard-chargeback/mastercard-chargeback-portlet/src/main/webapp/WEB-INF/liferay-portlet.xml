<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 7.4.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_7_4_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>mastercard-chargeback-portlet</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
		<private-request-attributes>false</private-request-attributes>
		<private-session-attributes>false</private-session-attributes>
		<requires-namespaced-parameters>false</requires-namespaced-parameters>
		<header-portlet-css>/css/chargeback.css</header-portlet-css>
		<header-portlet-css>/css/datePicker.css</header-portlet-css>
		<header-portlet-javascript>/js/jquery-1.3.2.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery.datePicker.js</header-portlet-javascript>
		<header-portlet-javascript>/js/date.js</header-portlet-javascript>
		<header-portlet-javascript>/js/chargeback.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery.simplemodal.js</header-portlet-javascript>
		<header-portlet-javascript>/js/json_parse.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery.print.js</header-portlet-javascript>
		<css-class-wrapper>mastercard-chargeback-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>