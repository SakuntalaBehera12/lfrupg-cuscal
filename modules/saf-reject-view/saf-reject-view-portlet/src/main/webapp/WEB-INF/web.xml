		<url-pattern>/WEB-INF/servlet/view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>safrejectviewportlet Servlet</servlet-name>
		<url-pattern>/safrejectviewportlet/*</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
