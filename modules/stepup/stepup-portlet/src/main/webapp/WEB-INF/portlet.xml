<?xml version="1.0"?>

<portlet-app version="3.0" xmlns="http://xmlns.jcp.org/xml/ns/portlet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/portlet http://xmlns.jcp.org/xml/ns/portlet/portlet-app_3_0.xsd">
	<portlet>
		<portlet-name>stepup</portlet-name>
		<display-name>stepup-portlet</display-name>
		<portlet-class>com.cuscal.security.twoFA</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/twoFA/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/twoFA/edit.jsp</value>
		</init-param>
		<init-param>
			<name>help-jsp</name>
			<value>/html/twoFA/help.jsp</value>
		</init-param>
		<init-param>
			<name>about-jsp</name>
			<value>/html/twoFA/about.jsp</value>
		</init-param>
		<init-param>
			<name>config-jsp</name>
			<value>/html/twoFA/config.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
			<portlet-mode>about</portlet-mode>
			<portlet-mode>config</portlet-mode>
			<window-state>normal</window-state>
		</supports>
		<portlet-info>
			<title>Stepup Portlet</title>
			<short-title>Stepup</short-title>
			<keywords></keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>Radius.Server.Auth.Port</name>
				<value>812</value>
			</preference>
			<preference>
				<name>Radius.Server.Acc.Port</name>
				<value>1813</value>
			</preference>
			<preference>
				<name>Radius.Server.Name</name>
				<value>168.217.134.11</value>
			</preference>
			<preference>
				<name>Radius.Server.Shared.Key</name>
				<value>cuscal</value>
			</preference>
			<preference>
				<name>Static.Password</name>
				<value>private</value>
			</preference>
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<container-runtime-option>
			<name>javax.portlet.actionScopedRequestAttributes</name>
			<value>true</value>
			<value>numberOfCachedScopes</value>
			<value>10</value>
		</container-runtime-option>
	</portlet>
	<custom-portlet-mode>
		<portlet-mode>view</portlet-mode>
		<portal-managed>false</portal-managed>
	</custom-portlet-mode>
	<custom-portlet-mode>
		<portlet-mode>edit</portlet-mode>
		<portal-managed>false</portal-managed>
	</custom-portlet-mode>
</portlet-app>